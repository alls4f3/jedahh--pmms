@{
    ViewData["Title"] = Serenity.LocalText.Get("Db.Default.WorkOrderOld.EntityPlural");
}
@section Head {

    <link rel="stylesheet" href="~/Content/geo/leaflet.css" />
    <script src="~/Content/geo/leaflet.js"></script>


}

<style>
    .ms-parent {
        width: 100% !important
    }

    .mslectmany {
        width: 100%;
        height: 27px !important;
    }

    .multiselect-container {
        height: 200px;
        overflow: scroll;
    }

    .rpcd {
        color: red;
    }

        .rpcd label {
            color: red;
        }


    .leaflet-container {
        height: 400px !important;
        width: 800px !important;
    }


    .leaflet-control-attribution {
        display: none;
    }


    body {
        padding-top: 60px;
        padding-bottom: 40px;
        height: 480px;
    }

    #mapviwer {
        height: 75%;
    }

    .fixed .content-wrapper, .fixed .right-side {
        padding-top: 0px;
    }

    .s-MessageDialog .ui-dialog-content {
        overflow: hidden;
    }

    .sigWrapper {
        clear: both;
        height: 91px;
        width: 300px;
        border: 1px solid #ccc;
    }

    .pad {
        padding: 0px;
    }
