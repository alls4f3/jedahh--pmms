<div id="GridDiv"></div>

<script type="text/javascript">
    jQuery(function () {
        new PMMS.Default.WorkOrderOldGrid($('#GridDiv'), {}).init();

        Q.initFullHeightGridPage($('#GridDiv'));
    });
</script>