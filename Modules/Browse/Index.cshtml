    function getSubDataSection(sectionNumber) {

        $.ajax({
            beforeSend: function () { Q.blockUI(null); }, complete: function () {
                Q.blockUndo();
            },
            type: "GET",
            url: "/Apps/Browse/GetSubData",
            data: {
                Section: sectionNumber,
    

