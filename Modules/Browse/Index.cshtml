                + '</td>'
                + '<td>'
                + '   <label>' + value.water_ponding + '</label>'
                + '</td>'
                + '<td>'
                + '   <label>' + value.utility_cuts + '</label>'
                + '</td>'
                + '<td>'
                + '   <label>' + value.Section_Rating +'</label>'
                + '</td>'
                + '<td>'
