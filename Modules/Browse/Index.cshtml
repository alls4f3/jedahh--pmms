<script>


    $(document).ready(function () {

          

        $("#divSectionData").hide();
        $("#divMedian").hide();

        $("#SubdivSectionData").hide();
        $("#SubdivMedian").hide();


        $.ajax({
            type: "GET",
            url: "/sysapi/getcity",
            data: {
                //  OID: orderid,


            },
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (result) {

                var dropdown = $('#CityID');
                var dropdownzone = $('#ZoneID');
                var dropdownarea = $('#AreaID');
                var dropRoadID = $('#RoadID');


                dropdown.empty();
                dropdownzone.empty();
                dropdownarea.empty();

                dropRoadID.empty();




                dropdown.append('<option selected="true" value="0">Please Select City</option>');
                dropdown.prop('selectedIndex', 0);


                $.each(result, function (key, entry) {
                    dropdown.append($('<option></option>').attr('value', entry.id).text(entry.Name));
                })




                $("#CityID").change(function () {
                    $.ajax({
                        type: "GET",
                        url: "/sysapi/getZone",
                        data: { id: $("#CityID").val() },
                        success: function (t) {


                            dropdownzone.empty();
                            dropdownarea.empty();
                            dropRoadID.empty();


                            dropdownzone.append('<option selected="true" value="0">Please Select  Zone</option>');
                            dropdownzone.prop('selectedIndex', 0);


                            $.each(t, function (key, entry) {
                                dropdownzone.append($('<option></option>').attr('value', entry.id).text(entry.Name));
                            })


                        }
                    });

                })



                $("#ZoneID").change(function () {
                    $.ajax({
                        type: "GET",
                        url: "/sysapi/getAreas",
                        data: { id: $("#ZoneID").val(), id2: $("#CityID").val() },
                        success: function (x) {
                            dropdownarea.empty();
                            dropRoadID.empty();


                            dropdownarea.append('<option selected="true" value="0">Select Area</option>');
                            dropdownarea.prop('selectedIndex', 0);





                            $.each(x, function (key, entry) {
                                dropdownarea.append($('<option></option>').attr('value', entry.id).text(entry.Name));
                            })



                        }
                    });

                })


                $("#AreaID").change(function () {

                    $.ajax({
                        type: "GET",
                        url: "/sysapi/getRoad",
                        data: { id: $("#CityID").val(), id2: $("#ZoneID").val(), id3: $("#AreaID").val() },
                        success: function (x) {

                            dropRoadID.empty();


                            dropRoadID.append('<option selected="true" value="0">Select Road</option>');
                            dropRoadID.prop('selectedIndex', 0);





                            $.each(x, function (key, entry) {
                                dropRoadID.append($('<option></option>').attr('value', entry.id).text(entry.Name));
                            })



                        }
                    });
                })




            }

            });


      

        function getrptpara() {


            var result = "";

            if ($("#CityID").val() <= 9) {
                result = result.concat("0", $("#CityID").val());
            }
            else {
                
                result = result.concat($("#CityID").val());

            }

            if ($("#ZoneID").val() != 0 && $("#ZoneID").val() != null) {
                if ($("#ZoneID").val() <= 9) {
                    result = result.concat("0", $("#ZoneID").val());
                }
                else {
                    result = result.concat($("#ZoneID").val());

                }
            }
          
            if ($("#AreaID").val() != 0 && $("#AreaID").val() != null) {

                if ($("#AreaID").val() > 0 && $("#AreaID").val() <= 9) {
                    result = result.concat("00", $("#AreaID").val());
                }
                else if ($("#AreaID").val() > 9 && $("#AreaID").val() <= 99) {
                    result = result.concat("0", $("#AreaID").val());
                }
                else {
                    result = result.concat($("#AreaID").val());

                }
            }

           
            if ($("#RoadID").val() != 0 && $("#RoadID").val()!=null) {
                if ($("#RoadID").val() > 0 && $("#RoadID").val() <= 9) {
                    result = result.concat("00", $("#RoadID").val());
                }
                else if ($("#RoadID").val() > 9 && $("#RoadID").val() <= 99) {
                    result = result.concat("0", $("#RoadID").val());
                }
                else {
                    result = result.concat($("#RoadID").val());

                }
            }

           

            return result;

        }


        $("#btnSectionData").click(function () {

            LoadSectionData() 
        })

        $("#btnMdianData").click(function () {

            LoadMdianData()
        })

        function LoadSectionData() {

            if ($("#CityID").val() != 0 && $("#CityID").val() != null) {


                $("#divSectionData").show();
                $("#divMedian").hide();

                $("#SubdivSectionData").show();
                $("#SubdivMedian").hide();

                var sectionnumber = getrptpara();
             

                Q.blockUI(null);
                $.post("/Apps/Browse/GetData", {
                    Section: sectionnumber,
                    road: $("#RoadClassID").val(),
                }, function (result) {
                    Q.blockUndo();

                        $("#SectionData").html(body1(result))

                });






            } else {
                Q.warning("Please select the City")

            }
        }



        function LoadMdianData() {

            if ($("#CityID").val() != 0 && $("#CityID").val() != null) {


                $("#divSectionData").hide();
                $("#divMedian").show();

                $("#SubdivSectionData").hide();
                $("#SubdivMedian").show();

                var sectionnumber = getrptpara();


                Q.blockUI(null);
                $.post("/Apps/Browse/GetData2", {
                    Section: sectionnumber,
                }, function (result) {
                    Q.blockUndo();

                        $("#MedianData").html(body2(result))

                });






            } else {
                Q.warning("Please select the City")

            }
        }
        
    })


    function body1(obj) {
