        function GetSectionNuber() {

            var result = "";

            if ($("#CityID").val() <= 9) {
                result = result.concat("", $("#CityID").val());
            }
            else {
                result = result.concat($("#CityID").val());

            }

            if ($("#ZoneID").val() <= 9) {
                result = result.concat("", $("#ZoneID").val());
            }
            else {
                result = result.concat($("#ZoneID").val());

            }


            if ($("#AreaID").val() > 0 && $("#AreaID").val() <= 9) {
                result = result.concat("", $("#AreaID").val());
            }
            else if ($("#AreaID").val() > 9 && $("#AreaID").val() <= 99) {
                result = result.concat("", $("#AreaID").val());
            }
            else {
                result = result.concat($("#AreaID").val());

            }


            if ($("#RoadID").val() > 0 && $("#RoadID").val() <= 9) {
                result = result.concat("", $("#RoadID").val());
            }
            else if ($("#RoadID").val() > 9 && $("#RoadID").val() <= 99) {
                result = result.concat("", $("#RoadID").val());
            }
            else {
                result = result.concat($("#RoadID").val());

            }

            if ($("#SectionID").val() > 0 && $("#SectionID").val() <= 9) {
                result = result.concat("", $("#SectionID").val());
            }
            else if ($("#SectionID").val() > 9 && $("#SectionID").val() <= 99) {
                result = result.concat("", $("#SectionID").val());
            }
            else {
                result = result.concat($("#SectionID").val());

            }



            result = result.concat($("#DirectionID").val());
            result = result.concat($("#FeatureID").val());
            result = result.concat("_");
            result = "";


            if ($("#SectionID").val() > 0 && $("#SectionID").val() <= 9) {
                result = result.concat("", $("#SectionID").val());
            }
            else if ($("#SectionID").val() > 9 && $("#SectionID").val() <= 99) {
                result = result.concat("", $("#SectionID").val());
            }
            else {
                result = result.concat($("#SectionID").val());

            }


            result = result.concat($("#DirectionID").val());
            result = result.concat($("#FeatureID").val());

            $("#SectionComapnion").val(result);

            return result;
        }

