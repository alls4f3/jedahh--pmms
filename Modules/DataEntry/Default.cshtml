            if ($("#Sample_ReadingSKID").val() != null && $("#Sample_ReadingSKID").val() != "") {
                SKIDD = $("#Sample_ReadingSKID").val();
            } else {
                SKIDD = -1;
            }


            if ($("#Sample_ReadingFWD").val() != null && $("#Sample_ReadingFWD").val() != "") {
                FWD = $("#Sample_ReadingFWD").val();
            } else {
                FWD = -1;
            }


            $.ajax({
                beforeSend: function () { Q.blockUI(null); }, complete: function () {
                    Q.blockUndo();
                },
                type: "GET",
                url: "/Apps/DataEntry/cdvcal",
                data: {

                    sumobj: total,
                    dctcnt: totaldctcnt,
                    IRI: IRI,
                    Skid: SKIDD,
                    Fwd: FWD,


                },

                contentType: 'application/json',
                success: function (result) {


                    $("#TOTAL_DEDUCT_VALUE").val(result.TotalDeductValue);
                    $("#COMPUTED_DEDUCT_VALUE").val(result.ComputedDeductValue);
                    $("#SAMPLE_PCI").val(result.SamplePCI);
                    $("#Rating").val(result.Rating);








                },



            })
        }

    })


</script>



